<h1>Hours</h1>
<div id="hoursWindowPrimer" class="windowPrimer"></div>
<div id="hoursWindow">
    <div id="hoursWindow-header">
        <div id="hoursWindow-title"> </div>
        <div id="hoursWindow-sectionTitle"> </div>
        <div id="hoursWindow-exit">x</div>
    </div>
    <div id="hoursWindow-body">
        <div id="hoursWindow-editDates">edit dates</div>
        <div>Start Date:</div>
        <div id="hoursWindow-hoursStartDate"> </div>
        <div>End Date:</div>
        <div id="hoursWindow-hoursEndDate"> </div>
        <div>Priority Number</div>
        <select id="hoursWindow-priorityNumberSelect">
            <option value="1">1</option>
            <option value="2">2</option>
        </select>
        <div id="hoursWindow-priorityNumberBase">0</div>
        <div id="hoursWindow-delete">DELETE</div>
        <div id="hoursWindow-timesHeader">Hours Of Operation</div>
        <ul id="hoursWindow-times">
            
        </ul>
    </div>
</div>


<div id="hoursEdit-editTimes" class="hoursEdit">
    <div class="hoursEdit-title">Title</div>
    <div class="hoursEdit-body">
        <div id="hoursEdit-startTimeTitle">Opens:</div>
        <div id="hoursEdit-startTimeSelect"> 
            <select></select>
            <span>:</span>
            <select></select>
            <select></select>
        </div>
        <div id="hoursEdit-endTimeTitle">Closes:</div>
        <div id="hoursEdit-endTimeSelect">
            <select></select>
            <span>:</span>
            <select></select>
            <select></select>
        </div>
    </div>
    <div class="hoursEdit-done">
        <input type="button" name="done" value="done" />
        <input type="button" name="cancel" value="cancel" />
    </div>
</div>
<div id="hoursEdit-editDates" class="hoursEdit" style="display: none;">
    <div class="hoursEdit-title"></div>
    <div class="hoursEdit-body"></div>
    <div class="hoursEdit-done"></div>
</div>

<div id="hoursWrapper">
    <div class="hoursSectionHeader">Default Hours</div>
    <div class="hoursSectionHeader-add">+</div>
    <ul id="defaultHours" class="hoursList">
        
    </ul>
    
    <div class="hoursSectionHeader">Facility Hours</div>
    <div class="hoursSectionHeader-add">+</div>
    <ul id="otherFacilityHours" class="hoursList">
        
    </ul>

    <div class="hoursSectionHeader">Closed Hours</div>
    <div class="hoursSectionHeader-add">+</div>
    <ul id="nonFacilityHours" class="hoursList">
        
    </ul>

    <div class="hoursSectionHeader">Other Hours</div>
    <div class="hoursSectionHeader-add">+</div>
    <ul id="closedHours" class="hoursList">

    </ul>
</div>

