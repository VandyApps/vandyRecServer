<h1>Intramurals</h1>

<div id="intramuralsImport">
	<form method="POST" action="/intramurals/files">
		<input type="file" name="intramuralsInput" />
		<input type="submit" />
	</form>
</div>

<div id="intramuralsWrapper">
    <div class="intramuralsSectionHeader">
        <div class="intramuralsSectionHeader-title">Fall Season</div>
    </div>
    <ul id="FallSeason" class="intramuralsList">
        
    </ul>
    
    <div class="intramuralsSectionHeader">
        <div class="intramuralsSectionHeader-title">Winter Season</div>
        <div class="intramuralsSectionHeader-add" id="hoursAdd-1">+</div>
    </div>
    <ul id="WinterSeason" class="intramuralsList">
        
    </ul>

    <div class="intramuralsSectionHeader">
        <div class="intramuralsSectionHeader-title">Spring Season</div>
        <div class="intramuralsSectionHeader-add" id="hoursAdd-2">+</div>
    </div>
    
    <ul id="SpringSeason" class="intramuralsList">
        
    </ul>

    <div class="intramuralsSectionHeader">
        <div class="intramuralsSectionHeader-title">Summer Season</div>
        <div class="intramuralsSectionHeader-add" id="hoursAdd-3">+</div>
    </div>
    <ul id="SummerSeason" class="intramuralsList">

    </ul>
</div>